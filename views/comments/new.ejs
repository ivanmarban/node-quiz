<div class="jumbotron">
  <h3>Add comment</h3>
  <form method="post" action="/quizzes/<%=quizid%>/comments/" >
    <input type="text" id="comment" class="form-control" name="comment[text]"/>
    <br/>
    <p>
      <input class="btn btn-sm btn-primary" type="submit" value="Submit">
      <input class="btn btn-sm btn-danger" type="button" onClick="window.history.go(-1); return false;" value="Cancel">
    </p>
  </form>
</div>
